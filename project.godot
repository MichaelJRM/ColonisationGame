[display]

window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="BaseBuilding"

[editor]

naming/scene_name_casing=1
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[filesystem]

import/fbx/enabled=false

